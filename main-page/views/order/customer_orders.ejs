<div class="container" >
<div class="col-md-9" id="customer-orders" >

        <div class="box">

            <div class="table-responsive">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <th>Mã đơn</th>
                            <th>Ngày</th>
                            <th>Tổng tiền</th>
                            <th>Trạng thái</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                    <%if (typeof listOrders == 'undefined') { %>
                       
                        <% }
                    else { %>
                        <% if (listOrders!==null) {
                            for(let i =0;i< listOrders.length;i++)
                            {%>
                                <tr>
                                    <th>#<%=listOrders[i].orderID %></th>
                                    <td><%=listOrders[i].orderTime %></td>
                                    <td><%=listOrders[i].orderID %></td>
                                    <% if (listOrders[i].status==="PROCESSING"){
                                        %><td><span class="label label-info">Soạn hàng</span></td><%
                                    }
                                    else{%>
                                        <% if(listOrders[i].status==="SHIPPING"){
                                            %><td><span class="label label-warning">Shipping</span></td><%
                                        }else{
                                            %><td><span class="label label-success">Hoàn tất</span</td><%
                                        } %>
                                    <%}
                                    %>
                                    <td><a href="/order/detail?id=<%=listOrders[i].orderID %>" class="aa-add-card-btn">Chi tiết</a>
                                    </td>
                                </tr>
            
                            <%}
                            }
                            else {%> <%}
                                %>
                        <%} %>
                    </tbody>
                </table>
            </div>
            <!-- /.table-responsive -->

        </div>
        <!-- /.box -->

    </div>
    <!-- /.col-md-9 -->

    <!-- *** LEFT COLUMN END *** -->


</div>


</div>
<!-- /.container -->
</div>
<!-- /#content -->
</div>
